{
  "version": "0.2.0",
  "configurations": [
    {
      // 调试回测
      "name": "Launcher Backtest",
      "type": "coreclr",
      "request": "launch",
      "program": "dotnet",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/Launcher/Launcher.fsproj",
        "--",
        "backtest",
        "-t", "Algorithm.MyAlgo",
        "-L", "FSharp",
        "-s", "2020-01-01",
        "-e", "2020-12-31",
        "-T", "${workspaceFolder}/Launcher/Configs/backtest.toml"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "stopAtEntry": false
    },
    {
      // 调试实盘
      "name": "Launcher Live",
      "type": "coreclr",
      "request": "launch",
      "program": "dotnet",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/Launcher/Launcher.fsproj",
        "--",
        "live",
        "-t", "Algorithm.MyLiveAlgo",
        "-L", "FSharp",
        "-b", "UnikoCrypto",
        "-q", "Lean.Extension.UnikoCryptoDataQueueHandler",
        "-T", "${workspaceFolder}/Launcher/Configs/live.toml"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "stopAtEntry": false
    }
  ]
}
